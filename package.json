{
  "name": "sub0",
  "version": "0.2.2",
  "description": "Clean minimalistic library to automate the unsubscription of RxJS like observables | No dependencies",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "url": "https://github.com/reimavronicolas/sub0"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "alsatian": "^3.2.1",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "clean": "shx rm -rf dist-umd dist-commonjs dist-esmodule",
    "build:dev": "rm -rf ./dist && webpack --config ./webpack.config.js --mode development",
    "build:prod": "rm -rf ./dist && webpack --config ./webpack.config.js --mode production",
    "build:cool": "npm run clean && tsc && tsc -m es6 --outDir dist && webpack",
    "test": "ts-node --project ./tsconfig.json ./run-test"
  },
  "keywords": [
    "unsubscribe",
    "rxjs",
    "angular",
    "observable"
  ],
  "author": "Rei Mavronicolas",
  "license": "ISC",
  "files": [
    "dist"
  ]
}
